<table class="table" summary="Selected company market by order">
    <thead>
    <tr>
        <th colspan="3">Buy</th>
    </tr>
    <tr>
        <th>#</th>
        <th>Volume</th>
        <th>Price</th>
    </tr>
    </thead>
    <tbody>

    {{#each bid in controller}}
        <tr>
            <td>#</td>
            <td>{{bid.volume}}</td>
            <td>{{bid.price}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>