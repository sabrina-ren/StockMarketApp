<h4>Market by Price</h4>

<table class="table table-bordered" summary="Selected company market by price">
    <thead>
    <tr>
        <th colspan="3">Buy</th>
        <th colspan="3">Sell</th>
    </tr>
    <tr>
        <th>#</th>
        <th>Volume</th>
        <th>Price</th>
        <th>Price</th>
        <th>Volume</th>
        <th>#</th>
    </tr>
    </thead>
    <tbody>

    {{#each line in sortedOrders}}
        <tr>
            <td>&nbsp{{line.bidNum}}</td>
            <td>{{line.bidVolume}}</td>
            <td>{{line.bidPrice}}</td>
            <td>{{line.sellPrice}}</td>
            <td>{{line.sellVolume}}</td>
            <td>{{line.sellNum}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>