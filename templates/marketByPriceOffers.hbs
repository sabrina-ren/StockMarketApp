<table class="table" summary="Selected company market by order">
    <thead>
    <tr>
        <th colspan="3">Sell</th>
    </tr>
    <tr>
        <th>Price</th>
        <th>Volume</th>
        <th>#</th>
    </tr>
    </thead>
    <tbody>

    {{#each offer in controller}}
        <tr>
            <td>{{offer.price}}</td>
            <td>{{offer.volume}}</td>
            <td>#</td>
        </tr>
    {{/each}}
    </tbody>
</table>